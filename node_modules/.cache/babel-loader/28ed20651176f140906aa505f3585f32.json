id=\"workout-name\"\n            v-model=\"workoutName\" />\n        </div>\n\n        <div class=\"flex flex-col\">\n          <label for=\"workout-type\" class=\"mb-1 text-sm text-at-light-green\"\n            >Workout Type</label\n          >\n          <select\n            class=\"p-2 text-grey-500 focus:outline-none\"\n            required\n            id=\"workout-type\"\n            v-model=\"workoutType\">\n            <option value=\"select-workout\">Select Workout</option>\n            <option value=\"strength\">Strength Training</option>\n            <option value=\"cardio\">Cardio Training</option>\n          </select>\n        </div>\n\n        <div v-if=\"workoutType === 'strength'\" class=\"flex flex-col gap-y-4\">\n          <div\n            class=\"flex flex-col gap-x-6 gap-y-2 relative md:flex-row\"\n            v-for=\"(item, index) in exercises\"\n            :key=\"index\">\n            <div class=\"flex flex-col md:w-1/3\">\n              <label\n                for=\"exercise-name\"\n                class=\"mb-1 text-sm text-at-light-green\"\n                >Exercise\n              </label>\n              <input\n                id=\"exercise-name\"\n                type=\"text\"\n                class=\"p-2 text-grey-500 focus:outline-none\"\n                v-model=\"item.exercise\"\n                required />\n            </div>\n            <div class=\"flex flex-col flex-1\">\n              <label for=\"sets\" class=\"mb-1 text-sm text-at-light-green\"\n                >Sets</label\n              >\n              <input\n                type=\"text\"\n                class=\"p-2 w-full text-grey-500 focus:outline-none\"\n                v-model=\"item.sets\"\n                required />\n            </div>\n\n            <div class=\"flex flex-col flex-1\">\n              <label for=\"reps\" class=\"mb-1 text-sm text-at-light-green\"\n                >Reps\n              </label>\n              <input\n                required\n                type=\"text\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                v-model=\"item.reps\" />\n            </div>\n\n            <div class=\"flex flex-col flex-1\">\n              <label for=\"weight\" class=\"mb-1 text-sm text-at-light-green\"\n                >Weight (LB's)\n              </label>\n              <input\n                required\n                type=\"text\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                v-model=\"item.weight\" />\n            </div>\n            <img\n              src=\"@/assets/images/trash-light-green.png\"\n              class=\"h-4 w-auto absolute -left-5 cursor-pointer\" />\n            <button\n              @click=\"addExercise\"\n              type=\"button\"\n              class=\"mt-6 py-2 px-6 rounded-sm text-sm self-start border-solid border-2 hover:border-at-light-green text-white bg-at-light-green border-transparent duration-200 hover:text-at-light-green hover:bg-white\">\n              Record Workout\n            </button>\n          </div>\n        </div>\n\n        <div v-if=\"workoutType === 'cardio'\" class=\"flex flex-col gap-y-4\">\n          <div\n            class=\"flex flex-col gap-x-6 gap-y-2 relative md:flex-row\"\n            v-for=\"(item, index) in exercises\"\n            :key=\"index\">\n            <div class=\"flex flex-col md:w-1/3\">\n              <label for=\"cardio-type\" class=\"mb-1 text-sm text-at-light-green\"\n                >Type\n              </label>\n              <select\n                id=\"cardio-type\"\n                class=\"p-2 w-full text-grey-500 focus:outline-none\"\n                v-model=\"item.cardioType\">\n                <option value=\"running\">Running</option>\n                <option value=\"walking\">Walking</option>\n              </select>\n            </div>\n            <div class=\"flex flex-col flex-1\">\n              <label for=\"distance\" class=\"mb-1 text-sm text-at-light-green\"\n                >Distance (kms)</label\n              >\n              <input\n                id=\"distance\"\n                type=\"text\"\n                class=\"p-2 w-full text-grey-500 focus:outline-none\"\n                v-model=\"item.distance\"\n                required />\n            </div>\n\n            <div class=\"flex flex-col flex-1\">\n              <label for=\"duration\" class=\"mb-1 text-sm text-at-light-green\"\n                >Duration (mins)\n              </label>\n              <input\n                id=\"duration\"\n                required\n                type=\"text\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                v-model=\"item.duration\" />\n            </div>\n\n            <div class=\"flex flex-col flex-1\">\n              <label for=\"pace\" class=\"mb-1 text-sm text-at-light-green\"\n                >Pace (km/hour)\n              </label>\n              <input\n                required\n                type=\"text\"\n                class=\"p-2 w-full text-gray-500 focus:outline-none\"\n                v-model=\"item.pace\" />\n            </div>\n            <img\n              src=\"@/assets/images/trash-light-green.png\"\n              class=\"h-4 w-auto absolute -left-5 cursor-pointer\" />\n            <button\n              @click=\"addExercise\"\n              type=\"button\"\n              class=\"mt-6 py-2 px-6 rounded-sm text-sm self-start border-solid border-2 hover:border-at-light-green text-white bg-at-light-green border-transparent duration-200 hover:text-at-light-green hover:bg-white\">\n              Record Workout\n            </button>\n          </div>\n        </div>\n\n        <button\n          @click=\"addExercise\"\n          type=\"submit\"\n          class=\"mt-6 py-2 px-6 rounded-sm text-sm self-start border-solid border-2 hover:border-at-light-green text-white bg-at-light-green border-transparent duration-200 hover:text-at-light-green hover:bg-white\">\n          Record Workout\n        </button>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { uid } from \"uid\";\nexport default {\n  name: \"create\",\n  setup() {\n    // Create data\n    const workoutName = ref(\"\");\n    const workoutType = ref(\"select-workout\");\n    const exercises = ref([1]);\n    const statusMessage = ref(null);\n    const errorMessage = ref(null);\n    // Add exercise\n    const addExercise = () => {\n      if (workoutType.value === \"strength\") {\n        exercises.value.push({\n          id: uid(),\n          exercise: \"\",\n          sets: \"\",\n          reps: \"\",\n          weight: \"\",\n        });\n        return;\n      }\n      exercises.value.push({\n        id: uid(),\n        cardioType: \"\",\n        distance: \"\",\n        duration: \"\",\n        pace: \"\",\n      });\n    };\n    // Delete exercise\n\n    const deleteExercise = (id) => {\n      if (exercises.value > 1) {\n        exercises.value = exercises.value.filter(\n          (exercise) => exercise.id !== id\n        );\n        return;\n      }\n\n      errorMessage.value =\n        \"Error: Can not be removed, need to have at least one exercise\";\n\n      setTimeout(() => {\n        errorMessage.value = null;\n      }, 3000);\n    };\n    // Listens for chaging of workout type input\n\n    // Create workout\n\n    return {\n      workoutName,\n      workoutType,\n      errorMessage,\n      exercises,\n      statusMessage,\n      addExercise,\n    };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}