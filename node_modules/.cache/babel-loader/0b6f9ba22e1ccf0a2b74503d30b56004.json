{"ast":null,"code":"import { ref } from \"vue\";\nimport { supabase } from \"../supabase/supabase\";\nexport default {\n  name: \"Home\",\n  components: {},\n\n  setup() {\n    // Create data / vars\n    const data = ref([]);\n    const dataLoaded = ref(null);\n\n    const getData = async () => {\n      try {\n        const {\n          data: workouts,\n          error\n        } = await supabase.from(\"workouts\").select(\"*\");\n        if (error) throw error;\n        data.value = workouts;\n        dataLoaded.value = true;\n        console.log(data.value);\n      } catch (error) {\n        console.log(error.message);\n      }\n    }; // Get data\n    // Run data function\n\n\n    getData();\n    return {\n      data,\n      dataLoaded\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA2CA,SAASA,GAAT,QAAoB,KAApB;AACA,SAASC,QAAT,QAAyB,sBAAzB;AACA,eAAe;EACbC,IAAI,EAAE,MADO;EAEbC,UAAU,EAAE,EAFC;;EAGbC,KAAK,GAAG;IACN;IACA,MAAMC,IAAG,GAAIL,GAAG,CAAC,EAAD,CAAhB;IACA,MAAMM,UAAS,GAAIN,GAAG,CAAC,IAAD,CAAtB;;IAEA,MAAMO,OAAM,GAAI,YAAY;MAC1B,IAAI;QACF,MAAM;UAAEF,IAAI,EAAEG,QAAR;UAAkBC;QAAlB,IAA4B,MAAMR,QAAO,CAC5CS,IADqC,CAChC,UADgC,EAErCC,MAFqC,CAE9B,GAF8B,CAAxC;QAGA,IAAIF,KAAJ,EAAW,MAAMA,KAAN;QACXJ,IAAI,CAACO,KAAL,GAAaJ,QAAb;QACAF,UAAU,CAACM,KAAX,GAAmB,IAAnB;QACAC,OAAO,CAACC,GAAR,CAAYT,IAAI,CAACO,KAAjB;MACF,CARA,CAQE,OAAOH,KAAP,EAAc;QACdI,OAAO,CAACC,GAAR,CAAYL,KAAK,CAACM,OAAlB;MACF;IACD,CAZD,CALM,CAkBN;IAEA;;;IACAR,OAAO;IAEP,OAAO;MAAEF,IAAF;MAAQC;IAAR,CAAP;EACD;;AA3BY,CAAf","names":["ref","supabase","name","components","setup","data","dataLoaded","getData","workouts","error","from","select","value","console","log","message"],"sourceRoot":"","sources":["C:\\Users\\Administrator\\Desktop\\Projet\\Vue\\workout-tracker\\src\\views\\Home.vue"],"sourcesContent":["<template>\n  <div v-if=\"dataLoaded\" class=\"container mt-10 px-4\">\n    <!-- No Data -->\n    <div v-if=\"data.length === 0\" class=\"w-full flex flex-col items-center\">\n      <h1 class=\"text-2xl\">Looks empty here...</h1>\n      <router-link\n        class=\"mt-6 py-2 px-6 rounded-sm text-sm self-start border-solid border-2 hover:border-at-light-green text-white bg-at-light-green border-transparent duration-200 hover:text-at-light-green hover:bg-white\">\n        :to=\"{name: 'Create'}\">Create Workout</router-link\n      >\n    </div>\n    <div\n      v-else\n      class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-6\">\n      <!-- Card -->\n      <router-link\n        class=\"flex flex-col items-center p-8 shadow-md bg-light-grey rounded-md cursor-pointer\"\n        :to=\"{ name: '' }\"\n        v-for=\"(workout, index) in data\"\n        :key=\"index\">\n        <!-- Cardio Image -->\n        <img\n          v-if=\"workout.workoutType === 'cardio'\"\n          class=\"h-24 w-auto\"\n          src=\"../assets/images/running-light-green.png\"\n          alt=\"\" />\n\n        <!-- Strength training -->\n        <img v-else src=\"../assets/images/dumbbell-light-green.png\" alt=\"\" />\n\n        <p\n          class=\"mt-6 py-1 px-3 text-xs text-white bg-at-light-green shadow-md rounded-lg\">\n          {{ workout.workoutType }}\n        </p>\n\n        <h1 class=\"mt-8 mb-2 text-center text-xl text-at-light-green\">\n          {{ workout.workoutName }}\n        </h1>\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref } from \"vue\";\nimport { supabase } from \"../supabase/supabase\";\nexport default {\n  name: \"Home\",\n  components: {},\n  setup() {\n    // Create data / vars\n    const data = ref([]);\n    const dataLoaded = ref(null);\n\n    const getData = async () => {\n      try {\n        const { data: workouts, error } = await supabase\n          .from(\"workouts\")\n          .select(\"*\");\n        if (error) throw error;\n        data.value = workouts;\n        dataLoaded.value = true;\n        console.log(data.value);\n      } catch (error) {\n        console.log(error.message);\n      }\n    };\n    // Get data\n\n    // Run data function\n    getData();\n\n    return { data, dataLoaded };\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}